#include <stdio.h>

void main()
{
    int arr[20];int n;int key;
    printf("enter the no of ele of std::array ");
    scanf("%d",&n);
    printf("enter the ele of arr");
    
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        printf("%d : %d",arr[i],i);
        
    }
    printf("the count is %d",n);
    printf("enter the ele to be searched for");
    scanf("%d",&key);
    for(int j=0;j<n;j++)
    {
        if(key==arr[j])
        {
            printf("key found at index %d",j);
            
            return;
        }
    }
    printf("unsuccessful\n");
    
}
